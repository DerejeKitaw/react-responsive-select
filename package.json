    "babel-core": "6.9.x",
    "babel-loader": "6.2.x",
    "babel-preset-es2015": "6.9.x",
    "babel-preset-react": "6.5.x",
    "babel-preset-stage-1": "6.5.x",
    "babel-polyfill": "6.22.x",

    "node-sass": "3.7.x",
